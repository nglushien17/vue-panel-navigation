{
  "name": "vue-panel-navigation",
  "version": "0.1.0",
  "description": "A vue fullscreen-esque navigation plugin",
  "main": "dist/index.js",
  "module": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "serve": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pxslip/vue-panel-navigation.git"
  },
  "keywords": [
    "vue",
    "panels",
    "navigation"
  ],
  "author": "Will Kruse <wskruse@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pxslip/vue-panel-navigation/issues"
  },
  "homepage": "https://github.com/pxslip/vue-panel-navigation#readme",
  "devDependencies": {
    "cross-env": "^5.1.4",
    "eslint": "^4.18.2",
    "husky": "^0.15.0-rc.12",
    "laravel-mix": "^2.1.9",
    "rollup": "^0.57.1"
  },
  "dependencies": {
    "lodash.get": "^4.4.2",
    "scrollmonitor": "^1.2.4",
    "shortid": "^2.2.8",
    "smoothscroll": "^0.4.0",
    "vue": "^2.5.16",
    "vue-directive-tooltip": "^1.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prod && git add dist/"
    }
  }
}
